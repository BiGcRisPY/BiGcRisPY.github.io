<div class="page-header">
  <h1>
    Tms
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>TM</th>
        <th>Move</th>
        <th>Effect</th>
      </tr>

      <% @tms.each do |tm| %>
      <tr>
        <td><%= tm.id %></td>
        <td><%= tm.move.name %></td>
        <td><%= tm.move.effect %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
