<div class="page-header">
  <h1>
    Learnedmoves
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Pokemon</th>
        <th>Level learned</th>
        <th>Move</th>
        <th>Effect</th>
      </tr>

      <% @learnedmoves.each do |learnedmove| %>
      <tr>
        <td><%= learnedmove.pokemon.creature %></td>
        <td><%= learnedmove.level_learned %></td>
        <td><%= learnedmove.move.name %></td>
        <td><%= learnedmove.move.effect %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
