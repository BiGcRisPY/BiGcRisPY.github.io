<div class="page-header">
  <h3>Learnedmove #<%= @learnedmove.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Pokemon</dt>
      <dd><%= @learnedmove.pokemon_id %></dd>

      <dt>Level learned</dt>
      <dd><%= @learnedmove.level_learned %></dd>

      <dt>Move</dt>
      <dd><%= @learnedmove.move_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/learnedmoves" class="btn btn-primary">
        Back
      </a>
      <a href="/learnedmoves/<%= @learnedmove.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_learnedmove/<%= @learnedmove.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
